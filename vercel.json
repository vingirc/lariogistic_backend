{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "docs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api-docs$",
      "dest": "/api-docs/",
      "status": 301
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "api/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/docs$",
      "dest": "/docs/",
      "status": 301
    },
    {
      "src": "/docs/$",
      "dest": "/docs/index.html"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "api/index.js"
    }
  ],
  "github": {
    "enabled": false
  },
  "env": {
    "NODE_ENV": "production"
  }
}